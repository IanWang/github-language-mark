{
	"name": "GitHub Language Mark",
	"version": "0.0.1",
	"description": "Show repo's language on your github dashboard",
	"homepage_url": "https://github.com/IanWang/github-language-mark",
	"manifest_version": 2,
	"minimum_chrome_version": "36",
	"icons": {
	},
	"permissions": [
		"https://github.com/"
	],
	"content_scripts": [
		{
			"run_at": "document_end",
			"matches": [
				"https://github.com/"
			],
			"css": [
				"content.css"
			],
			"js": [
				"content.js"
			]
		}
	]
}
